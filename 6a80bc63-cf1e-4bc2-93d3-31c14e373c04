<script type="text/javascript">
  function setAlbumState(sortby, pagenum, pagesize, singlephoto) {
    createCookie("sortBy", sortby, 0);
    createCookie("page", pagenum, 0);
    createCookie("pageSize", pagesize, 0);
    createCookie("singlePhoto", singlephoto, 0);
    return true;
  }
</script>

    







		
	
	
				








<script type="text/javascript">
  function removeComment( commentId )
  {
      var hidden = document.getElementById("removeCommentId");
      if(hidden) {
          hidden.value = commentId;
          document.getElementById("removePhotoCommentForm").submit();
      }
  }
  
  // check if privacy bit is set before submitting form
  function doMainPhotoSubmit(actionUrl) {
    var submitOK = true;
    var form = document.getElementById('privacyForm');    
    var confMsg = "Making this your Main Photo will automatically make this photo publicly viewable.  Are you sure you would like to proceed?";
    
    // get value of currently selected radio button
    for(var i=0;i < form.elements["photo_priv"].length; i++) {
      var myRadio = form.elements["photo_priv"][i];
      // if "Private" is selected popup confirmation message
      if(myRadio.checked && myRadio.value=="true") {
        submitOK = confirm(confMsg);
      }
    }
    if(submitOK) {
      document.photoControlForm.action=actionUrl;
      document.photoControlForm.submit();            
    }
    return false;
  }
  
</script>


